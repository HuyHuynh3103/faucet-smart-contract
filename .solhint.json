{
  "extends": "solhint:recommended",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "var-name-mixedcase": "none",
    "prettier/prettier": "error",
    "no-empty-blocks": "none",
	"no-console": "none",
	"no-global-import": "none",
    "compiler-version": [
      "error",
      "^0.8.17"
    ],
	"reason-string": "none"
  }
}